        };



        const employeeMenu = document.getElementById('employee-menu');

        const signatureDetails = document.getElementById('signature-details');

        const employeeNameTitle = document.getElementById('employee-name-title');

        const signaturePreview = document.getElementById('signature-preview');

        const signatureCode = document.getElementById('signature-code');

        const copyPreviewBtn = document.getElementById('copy-preview-btn');

        const copyCodeBtn = document.getElementById('copy-code-btn');

        const initialMessage = document.getElementById('initial-message');

        const alertMessage = document.getElementById('alert-message');



        let currentSignatureHTML = "";



        // Function to populate the sidebar menu

        function populateMenu() {

            const names = Object.keys(signatures).sort();

            names.forEach(name => {

                const li = document.createElement('li');

                const a = document.createElement('a');

                a.href = "#";

                a.textContent = `${name} (${signatures[name].title})`;

                a.setAttribute('data-name', name);

                a.addEventListener('click', (e) => {

                    e.preventDefault();

                    displaySignature(name);

                    // Update active state

                    document.querySelectorAll('.employee-list a').forEach(link => link.classList.remove('active'));

                    a.classList.add('active');

                });

                li.appendChild(a);

                employeeMenu.appendChild(li);

            });

        }



        // Function to display the selected signature details

        function displaySignature(name) {

            const data = signatures[name];

            currentSignatureHTML = data.html;



            initialMessage.style.display = 'none';

            signatureDetails.style.display = 'block';

            employeeNameTitle.textContent = `${name} (${data.title})`;

            signaturePreview.innerHTML = currentSignatureHTML;

            signatureCode.value = currentSignatureHTML;

        }



        // Function to show a temporary success alert

        function showAlert(message) {

            alertMessage.textContent = message;

            alertMessage.style.display = 'block';

            setTimeout(() => {

                alertMessage.style.display = 'none';

            }, 3000);

        }



        // Copy raw HTML code

        copyCodeBtn.addEventListener('click', () => {

            if (currentSignatureHTML) {

                signatureCode.select();

                document.execCommand('copy');

                showAlert('Raw HTML Source Code copied!');

            }

        });



        // Copy rich HTML/Preview (the tricky part for Outlook/Gmail)

        copyPreviewBtn.addEventListener('click', () => {

            if (currentSignatureHTML) {

                // Use the modern clipboard API if available

                if (navigator.clipboard && navigator.clipboard.write) {

                    const blob = new Blob([currentSignatureHTML], { type: 'text/html' });

                    const clipboardItem = new ClipboardItem({ 'text/html': blob });

                    navigator.clipboard.write([clipboardItem]).then(() => {

                        showAlert('Signature Preview (Rich Text/HTML) copied!');

                    }).catch(err => {

                        console.error('Could not copy Rich Text to clipboard using API. Falling back to execCommand.', err);

                        // Fallback method for rich text copy (relies on selecting and executing copy)

                        fallbackCopyRichText();

                    });

                } else {

                    // Fallback for older browsers

                    fallbackCopyRichText();

                }

            }

        });



        function fallbackCopyRichText() {

            // Temporarily select the content of the signature preview div

            const range = document.createRange();

            range.selectNode(signaturePreview);

            window.getSelection().removeAllRanges();

            window.getSelection().addRange(range);
